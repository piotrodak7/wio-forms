    "FieldValidatorsJS": {},
    "FieldValidatorsPHP": {
        "NotEmpty": {
            "class": "NotEmpty"
        }
    },
    "ContainerValidatorsPHP": {
        "AllFieldsOk": {
            "class": "AllFieldsOk"
        },
        "FormSaverOk": {
            "class": "FormSaverOk"
        }
    },
    "ContainerValidatorsJS": {},
    "Fields": {
        "userId": {
            "type": "Hidden",
            "container": "bigFormFront",
            "position": 1,
            "title": "User Id",
            "state": 0,
            "save": true,
            "defaultValue": {
                "repositoryName": "userId"
            }
        },
        "editionSzPCount": {
            "type": "Select",
            "container": "bigFormFront",
            "position": 2,
            "title": "W ilu Edycjach SZLACHETNEJ PACZKI brałeś udział",
            "state": 0,
            "save": true,
            "dataSet": {
                "repositoryName": "editionCounter"
            },
            "class" : {
                "wrapper": "edititem half-l",
                "input" : "veryLong wypisz"
            },
            "validationPHP":
            [
                {
                    "method": "NotEmpty"
                }
            ]
        },
        "editionAPCount": {
            "type": "Select",
            "container": "bigFormFront",
            "position": 3,
            "title": "W ilu Edycjach AKADEMII PRZYSZŁOŚCI brałeś udział",
            "state": 0,
            "save": true,
            "dataSet": {
                "repositoryName": "editionCounter"
            },
            "class" : {
                "wrapper": "edititem half-l",
                "input" : "veryLong wypisz"
            }
        },
        "previousProgram": {
            "type": "Select",
            "container": "bigFormFront",
            "position": 4,
            "title": "Program",
            "state": 0,
            "save": true,
            "dataSet": {
                "repositoryName": "programType"
            },
            "class" : {
                "wrapper": "edititem half-l",
                "input" : "veryLong wypisz"
            },
            "isGroup" : true
        },
        "previousRole": {
            "type": "Select",
            "container": "bigFormFront",
            "position": 5,
            "title": "Rola",
            "state": 0,
            "save": true,
            "dataSet": {
                "repositoryName": "roles"
            },
            "dynamicOptions": "previousProgram",
            "class" : {
                "wrapper": "edititem half-l",
                "input" : "veryLong wypisz"
            },
            "isGroup" : true
        },
        "dupliacte": {
            "type": "Duplicator",
            "duplicate" : ["previousProgram", "previousRole"],
            "container": "bigFormFront",
            "position": 6,
            "title": "+",
            "state": 0,
            "save": false,
            "class" : {
                "wrapper": "edititem half-l",
                "input" : "veryLong wypisz"
            },
            "dataSet": {
                "repositoryName" : ""
            }
        }
    },
    "Containers": {
        "Site1": {
            "site": 0,
            "container": "_site",
            "position": 1,
            "displayType": "FullDisplay",
            "title": "Historia zaangażowania"
        },
        "Site2": {
            "site": 1,
            "container": "_site",
            "position": 1,
            "displayType": "FullDisplay",
            "title": "Zadania",
            "validationPHP":
            [
                {
                    "type": "method",
                    "method": "AllFieldsOk",
                    "settings":
                    {
                        "containerToCheck": "_site_0"
                    }
                }
            ],
            "stateActions":
            [
                {
                    "state": -1,
                    "actions": ["hide"]
                },
                {
                    "state": 1,
                    "actions": ["show"]
                }
            ]
        },
        "Site3": {
            "site": 2,
            "container": "_site",
            "position": 1,
            "displayType": "FullDisplay",
            "title": "Doświadczenie",
            "validationPHP":
            [
                {
                    "type": "method",
                    "method": "AllFieldsOk",
                    "settings":
                    {
                        "containerToCheck": "_site_0"
                    }
                }
            ],
            "stateActions":
            [
                {
                    "state": -1,
                    "actions": ["hide"]
                },
                {
                    "state": 1,
                    "actions": ["show"]
                }
            ]
        },
        "bigFormFront": {
            "site": 0,
            "container": "Site2",
            "position": 1,
            "displayType": "SectionDisplay",
            "title": ""
        },
        "SubmitButton": {
            "site": 0,
            "container": "Site2",
            "position": 2,
            "displayType": "Button1",
            "title": "Dalej"
        },
        "bigFormFront2": {
            "site": 1,
            "container": "Site3",
            "position": 1,
            "displayType": "SectionDisplay",
            "title": "Zadania"
        },
        "SubmitButton2": {
            "site": 1,
            "container": "Site3",
            "position": 2,
            "displayType": "Button1",
            "title": "Dalej"
        }
    },
    "FormSavers": {
        "finalSave": {
            "clearTemporarySave": true,
            "databaseConnection": "Main",
            "DatabaseSaves": [
            ]
        }
    }
}
